
@{
    ViewBag.Title = "Stats";
}



<div class="container">
    <div class="row text-center">
        <h2>Stats</h2>
    </div>
    
    <hr />
    <div class="row">
        <div class="col-lg-12 text-center">
            Pie chart, Daily activities
            https://developers.google.com/chart/interactive/docs/gallery/piechart
        </div>
    </div>
    <hr />
    <div class="row">
        <div class="col-lg-12">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th scope="col">Category</th>
                        <th scope="col">Average Time</th>
                        <th scope="col">Last Update</th>
                        <th scope="col">Stats</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Working</td>
                        <td>6 hours</td>
                        <td>Last Added Date</td>
                        <td>Line chart https://developers.google.com/chart/interactive/docs/gallery/linechart </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>


